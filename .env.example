# 前端環境變量
REACT_APP_API_URL=http://localhost:8000/api
REACT_APP_APP_NAME=櫻色富士

# n8n Webhook URL（前端用）
VITE_N8N_WEBHOOK_URL=http://localhost:5678/webhook

# Google Gemini API（前端 AI 功能用）
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# ===========================================
# n8n 後端環境變數（在 n8n 中設定）
# ===========================================

# ExchangeRate-API（匯率監控）
EXCHANGE_RATE_API_KEY=your_exchangerate_api_key

# OpenWeatherMap API（天氣預報）
OPENWEATHERMAP_API_KEY=your_openweathermap_api_key

# Google API（Sheets, Calendar）
GOOGLE_SERVICE_ACCOUNT_EMAIL=your_service_account@project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY=your_private_key

# Strava API（跑步數據同步）
STRAVA_CLIENT_ID=your_strava_client_id
STRAVA_CLIENT_SECRET=your_strava_client_secret
STRAVA_ACCESS_TOKEN=your_strava_access_token
STRAVA_REFRESH_TOKEN=your_strava_refresh_token

# Telegram Bot（通知推播）
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Line Notify（可選，替代 Telegram）
LINE_NOTIFY_TOKEN=your_line_notify_token

# Notion API（訓練記錄存儲）
NOTION_API_KEY=your_notion_integration_token
NOTION_DATABASE_ID=your_notion_database_id

# Todoist API（打包清單管理）
TODOIST_API_TOKEN=your_todoist_api_token
TODOIST_PROJECT_ID=your_todoist_project_id

# Google Sheets ID（訓練記錄存儲）
GOOGLE_SHEET_ID=your_google_sheet_id
